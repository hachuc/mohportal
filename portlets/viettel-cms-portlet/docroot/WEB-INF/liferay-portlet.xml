<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>viettel-cms</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>viettel-cms-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>category-mngt</portlet-name>
		<icon>/images/icon/cat-icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>category-mngt-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>newcontent</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>newcontent-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>content-list</portlet-name>
		<icon>/icon.png</icon>		
		<configuration-action-class>com.viettel.portal.vcms.contentlist.action.ConfigurationActionImpl</configuration-action-class>
		<action-url-redirect>true</action-url-redirect>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/css/list-content.css</header-portlet-css>
		<header-portlet-css>/css/chosen.min.css</header-portlet-css>
		<header-portlet-javascript>
			/js/jquery-3.2.1.min.js
		</header-portlet-javascript>
		<footer-portlet-javascript>
			/js/chosen.jquery.min.js
		</footer-portlet-javascript>
		<footer-portlet-javascript>
			/js/diff_match_patch.js
		</footer-portlet-javascript>
		<css-class-wrapper>content-list-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>content-list-viewer</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			content-list-viewer-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>content-publisher</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>com.viettel.portal.vcms.publisher.ConfigurationActionImpl</configuration-action-class>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>2018</friendly-url-mapping>
		<friendly-url-routes>com/viettel/portal/vcms/publisher/vcms-publisher-friendly-url-routes.xml</friendly-url-routes>
		<header-portlet-css>/css/abstract.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>content-publisher-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>Display Content</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>com.viettel.portal.vcms.displaycontent.ConfigurationActionImpl</configuration-action-class>
		<instanceable>true</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/css/chosen.min.css</header-portlet-css>
		<header-portlet-javascript>
			/js/jquery-3.2.1.min.js
		</header-portlet-javascript>
		<footer-portlet-javascript>
			/js/chosen.jquery.min.js
		</footer-portlet-javascript>
		<css-class-wrapper>display-content-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>Migrate</portlet-name>
		<icon>/icon.png</icon>
		<control-panel-entry-category>site_administration.content</control-panel-entry-category>
		<control-panel-entry-weight>4.5</control-panel-entry-weight>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>migrate-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>content-type</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>content-type-portlet</css-class-wrapper>
	</portlet>
	<portlet>
        <portlet-name>Comment</portlet-name>
        <icon>/icon.png</icon>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>
            /js/main.js
        </footer-portlet-javascript>
        <css-class-wrapper>comment-portlet</css-class-wrapper>
    </portlet>
	<portlet>
		<portlet-name>most-viewed</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>com.viettel.portal.vcms.mostview.ConfigurationActionImpl</configuration-action-class>
		<instanceable>true</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>most-viewed-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>