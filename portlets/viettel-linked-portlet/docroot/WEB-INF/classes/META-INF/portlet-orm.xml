<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.viettel.portal.links.model.impl.LCategoryModelImpl">
		<attributes>
			<id name="categoryId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="categoryName" />
			<basic name="corder" />
			<basic name="isActive" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.viettel.portal.links.model.impl.VLinkModelImpl">
		<attributes>
			<id name="linkId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="linkName" />
			<basic name="url" />
			<basic name="isActive" />
			<basic name="corder" />
			<basic name="categoryId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.viettel.portal.links.model.impl.LCategoryImpl" name="LCategory">
		<table name="portal_linkcategory" />
		<attributes />
	</entity>
	<entity class="com.viettel.portal.links.model.impl.VLinkImpl" name="VLink">
		<table name="portal_link" />
		<attributes />
	</entity>
</entity-mappings>