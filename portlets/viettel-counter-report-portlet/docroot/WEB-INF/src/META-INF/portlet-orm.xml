<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.viettel.portal.counter.model.impl.CategoryModelImpl">
		<attributes>
			<id name="categoryId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="categoryName" />
			<basic name="categoryDesc" />
			<basic name="sortOrder" />
			<basic name="archived" />
			<basic name="layoutId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.viettel.portal.counter.model.impl.CounterReportModelImpl">
		<attributes>
			<id name="reportId" />
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.viettel.portal.counter.model.impl.CategoryImpl" name="Category">
		<table name="vcms_Category" />
		<attributes />
	</entity>
	<entity class="com.liferay.viettel.portal.counter.model.impl.CounterReportImpl" name="CounterReport">
		<table name="CounterReport" />
		<attributes />
	</entity>
</entity-mappings>